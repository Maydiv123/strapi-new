{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "blocks"
    },
    "short_description": {
      "type": "text"
    },
    "service_category": {
      "type": "enumeration",
      "enum": [
        "ai_services",
        "app_development",
        "web_development",
        "marketing",
        "ui_ux_design",
        "consulting"
      ]
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pricing_plans": {
      "type": "component",
      "component": "pricing.pricing-plan",
      "repeatable": true
    },
    "features": {
      "type": "json"
    },
    "technologies": {
      "type": "json"
    },
    "is_featured": {
      "type": "boolean"
    },
    "order": {
      "type": "integer"
    },
    "seo": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::seo-metadata.seo-metadata"
    },
    "portfolios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::portfolio.portfolio",
      "mappedBy": "services"
    },
    "testimonials": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::testimonial.testimonial",
      "mappedBy": "services"
    }
  }
}
